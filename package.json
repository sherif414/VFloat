{
  "name": "v-float",
  "version": "0.1.0",
  "description": "Vue 3 port of Floating UI - a library for positioning floating elements",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/src/index.d.ts",
  "main": "./dist/v-float.umd.js",
  "module": "./dist/v-float.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/v-float.es.js",
      "require": "./dist/v-float.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --no-watch",
    "build-only": "vite build",
    "type-check": "vue-tsc --build tsconfig.build.json",
    "lint": "biome check --apply --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@vueuse/core": "^13.0.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "lucide-vue-next": "^0.513.0",
    "@biomejs/biome": "1.9.4",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.14",
    "@unocss/reset": "66.1.0-beta.7",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "3.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "typescript": "~5.7.3",
    "unocss": "66.1.0-beta.7",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.9",
    "vitest-browser-vue": "^0.2.0",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "esbuild",
      "msw"
    ]
  },
  "packageManager": "pnpm@10.6.0"
}