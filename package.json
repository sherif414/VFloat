{
  "name": "v-float",
  "version": "0.7.0",
  "description": "Vue 3 port of Floating UI - a library for positioning floating elements",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/src/index.d.ts",
  "main": "./dist/v-float.umd.js",
  "module": "./dist/v-float.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/v-float.es.js",
      "require": "./dist/v-float.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build tsconfig.build.json",
    "lint": "biome check --write",
    "test": "vitest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:dry": "release-it --dry-run"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@vueuse/core": "^13.7.0",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@21st-extension/toolbar-vue": "^0.5.14",
    "@21st-extension/vue": "^0.5.14",
    "@biomejs/biome": "2.2.2",
    "@release-it/conventional-changelog": "^10.0.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.0",
    "@unocss/reset": "66.1.0-beta.7",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.1.0",
    "lucide-vue-next": "^0.513.0",
    "npm-run-all2": "^7.0.2",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "release-it-pnpm": "^4.6.6",
    "typescript": "~5.7.3",
    "unocss": "66.1.0-beta.7",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitepress": "^1.6.4",
    "vitepress-plugin-demo": "^0.8.1",
    "vitest": "^3.2.4",
    "vitest-browser-vue": "^0.2.0",
    "vue-tsc": "^2.2.12"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "esbuild",
      "msw"
    ]
  },
  "packageManager": "pnpm@10.6.0"
}
