{
  "name": "v-float",
  "version": "0.10.0",
  "description": "Vue 3 port of Floating UI - a library for positioning floating elements",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/src/index.d.ts",
  "main": "./dist/v-float.umd.js",
  "module": "./dist/v-float.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/v-float.es.js",
      "require": "./dist/v-float.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build tsconfig.build.json",
    "lint": "biome check --write",
    "test": "vitest run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:dry": "release-it --dry-run",
    "changelog": "changelogen",
    "changelog:preview": "changelogen --no-output"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@vueuse/core": "^14.0.0",
    "focus-trap": "^7.6.6",
    "tabbable": "^6.3.0",
    "unocss": "^66.5.9",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.19.1",
    "@unocss/reset": "66.1.0-beta.7",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/browser": "^4.0.13",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "@vue/tsconfig": "^0.8.1",
    "changelogen": "^0.6.2",
    "jsdom": "^27.2.0",
    "lucide-vue-next": "^0.554.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "release-it": "^19.0.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^1.6.4",
    "vitepress-plugin-demo": "^0.8.1",
    "vitest": "^4.0.13",
    "vitest-browser-vue": "^2.0.1",
    "vue-tsc": "^3.1.5"
  },
  "packageManager": "npm@11.6.3"
}
